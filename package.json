{
  "name": "crds-connect",
  "version": "1.0.0",
  "scripts": {
    "serve": "npm run start",
    "start": "webpack-dev-server --inline --open --progress --port 8080",
    "test": "npm jest",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "build-dev": "rimraf dist && rimraf node_modules/@types/jasmine && webpack --config config/webpack.dev.js --progress --profile --bail",
    "build-int": "rimraf dist && rimraf node_modules/@types/jasmine && webpack --config config/webpack.int.js --progress --profile --bail",
    "build": "rimraf dist && rimraf node_modules/@types/jasmine webpack --config config/webpack.prod.js --progress --profile --bail"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/angular/angular.io/blob/master/LICENSE"
    }
  ],
  "private": true,
  "dependencies": {
    "@angular/common": "2.2.3",
    "@angular/compiler": "2.2.3",
    "@angular/core": "2.2.3",
    "@angular/forms": "2.2.3",
    "@angular/http": "2.2.3",
    "@angular/platform-browser": "2.2.3",
    "@angular/platform-browser-dynamic": "2.2.3",
    "@angular/router": "3.2.3",
    "@types/lodash": "ts2.0",
    "angular2-cookie": "1.2.5",
    "angular2-google-maps": "0.17.0",
    "angular2-select": "1.0.0-beta.3",
    "angulartics2": "1.5.2",
    "copyfiles": "1.0.0",
    "core-js": "2.4.1",
    "crds-styles": "0.5.0",
    "iframe-resizer": "3.5.9",
    "intl": "1.2.5",
    "moment": "2.17.0",
    "ng2-bootstrap": "1.1.16",
    "ng2-toastr": "^1.6.0",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "1.1.2",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/jest": "^19.2.2",
    "@types/node": "6.0.68",
    "angular-cli": "1.0.0-beta.18",
    "angular2-template-loader": "0.5.0",
    "awesome-typescript-loader": "3.1.3",
    "bootstrap-sass": "3.3.7",
    "codelyzer": "2.0.0",
    "copy-webpack-plugin": "4.0.0",
    "crds-ng2-content-block": "1.0.4",
    "css-loader": "0.25.0",
    "css-to-string-loader": "0.1.2",
    "dotenv-webpack": "1.4.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.24.0",
    "jest": "^19.0.2",
    "jest-preset-angular": "^1.2.0",
    "jest-teamcity-reporter": "^0.6.2",
    "jest-zone-patch": "0.0.6",
    "node-sass": "3.10.1",
    "null-loader": "0.1.1",
    "open": "0.0.5",
    "protractor": "4.0.10",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "ts-jest": "^19.0.14",
    "ts-node": "1.6.1",
    "tslint": "4.1.1",
    "tslint-loader": "3.3.0",
    "typescript": "2.3.2",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "0.15.0"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json",
      "__TRANSFORM_HTML__": true
    },
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/config/",
      "webpack.test.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!crds-ng2-content-block)"
    ],
    "testResultsProcessor": "jest-teamcity-reporter"
  }
}
